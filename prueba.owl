<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:ex="http://example.org/ciudades#">

  <!-- Clases -->
  <rdf:Description rdf:about="http://example.org/ciudades#Ciudad">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="es">Ciudad</rdfs:label>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/ciudades#Provincia">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="es">Provincia</rdfs:label>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/ciudades#Pais">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="es">País</rdfs:label>
  </rdf:Description>

  <!-- Propiedades -->
  <rdf:Description rdf:about="http://example.org/ciudades#perteneceA">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="http://example.org/ciudades#Ciudad"/>
    <rdfs:range rdf:resource="http://example.org/ciudades#Provincia"/>
    <rdfs:label xml:lang="es">pertenece a</rdfs:label>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/ciudades#ubicadoEn">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="http://example.org/ciudades#Provincia"/>
    <rdfs:range rdf:resource="http://example.org/ciudades#Pais"/>
    <rdfs:label xml:lang="es">ubicado en</rdfs:label>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/ciudades#esDe">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdfs:domain rdf:resource="http://example.org/ciudades#Ciudad"/>
    <rdfs:range rdf:resource="http://example.org/ciudades#Pais"/>
    <rdfs:label xml:lang="es">es de</rdfs:label>
  </rdf:Description>

  <!-- Individuos -->
  <rdf:Description rdf:about="http://example.org/ciudades#España">
    <rdf:type rdf:resource="http://example.org/ciudades#Pais"/>
    <rdfs:label xml:lang="es">España</rdfs:label>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/ciudades#Sevilla">
    <rdf:type rdf:resource="http://example.org/ciudades#Ciudad"/>
    <rdfs:label xml:lang="es">Sevilla</rdfs:label>
    <ex:perteneceA rdf:resource="http://example.org/ciudades#ProvinciaSevilla"/>
    <ex:esDe rdf:resource="http://example.org/ciudades#España"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/ciudades#ProvinciaSevilla">
    <rdf:type rdf:resource="http://example.org/ciudades#Provincia"/>
    <rdfs:label xml:lang="es">Provincia de Sevilla</rdfs:label>
    <ex:ubicadoEn rdf:resource="http://example.org/ciudades#España"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/ciudades#Bilbao">
    <rdf:type rdf:resource="http://example.org/ciudades#Ciudad"/>
    <rdfs:label xml:lang="es">Bilbao</rdfs:label>
    <ex:perteneceA rdf:resource="http://example.org/ciudades#ProvinciaVizcaya"/>
    <ex:esDe rdf:resource="http://example.org/ciudades#España"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/ciudades#ProvinciaVizcaya">
    <rdf:type rdf:resource="http://example.org/ciudades#Provincia"/>
    <rdfs:label xml:lang="es">Provincia de Vizcaya</rdfs:label>
    <ex:ubicadoEn rdf:resource="http://example.org/ciudades#España"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/ciudades#Málaga">
    <rdf:type rdf:resource="http://example.org/ciudades#Ciudad"/>
    <rdfs:label xml:lang="es">Málaga</rdfs:label>
    <ex:perteneceA rdf:resource="http://example.org/ciudades#ProvinciaMalaga"/>
    <ex:esDe rdf:resource="http://example.org/ciudades#España"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/ciudades#ProvinciaMalaga">
    <rdf:type rdf:resource="http://example.org/ciudades#Provincia"/>
    <rdfs:label xml:lang="es">Provincia de Málaga</rdfs:label>
    <ex:ubicadoEn rdf:resource="http://example.org/ciudades#España"/>
  </rdf:Description>

</rdf:RDF>
